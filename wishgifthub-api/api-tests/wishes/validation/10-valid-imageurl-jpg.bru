meta {
  name: 10 - Image URL valide avec extension jpg
  type: http
  seq: 10
}

post {
  url: {{base_url}}/api/wishes
  body: json
  auth: bearer
}

auth:bearer {
  token: {{admin_token}}
}

body:json {
  {
    "giftName": "Test image URL valide jpg",
    "description": "Test avec une URL d'image valide se terminant par .jpg",
    "url": "https://example.com/produit",
    "imageUrl": "https://example.com/image.jpg",
    "price": "29.99 €"
  }
}

assert {
  res.status: eq 201
}

tests {
  test("Le souhait doit être créé avec une image URL valide (.jpg)", function() {
    expect(res.status).to.equal(201);
    expect(res.body.imageUrl).to.equal("https://example.com/image.jpg");
  });
}

